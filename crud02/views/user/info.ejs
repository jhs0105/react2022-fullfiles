<%- include("../include/header") %>
<h2>회원정보</h2>
<div class="user-info">
  <dl>
    <dt>아이디</dt>
    <dd><%= userInfo.id %></dd>
  </dl>
  <dl>
    <dt>이름</dt>
    <dd><%= userInfo.name %></dd>
  </dl>
  <dl>
    <dt>연락처</dt>
    <dd><%= userInfo.phone %></dd>
  </dl>
  <dl>
    <dt>비밀번호</dt>
    <dd><%= userInfo.password %></dd>
  </dl>
  <dl>
    <dt>주소</dt>
    <dd><%= userInfo.address %></dd>
  </dl>
  <div class="btns">
    <a href="/user/modify" class="btn btn-confirm">회원정보수정</a>
    <a href="/" class="btn btn-confirm">main</a>
    <a href="/user/logout" class="btn btn-confirm">로그아웃</a>
  </div>
</div>

<%- include("../include/footer") %>
